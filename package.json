{
  "name": "generadorcv",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/goonfarias11/generadorcv.git"
  },
  "author": "goonfarias11",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:prod": "next build && npm run lighthouse",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:visual": "playwright test --config=playwright.visual.config.js",
    "test:visual:update": "playwright test --config=playwright.visual.config.js --update-snapshots",
    "test:ci": "npm run test:unit && npm run test:e2e",
    "test:all": "npm run test && npm run test:e2e && npm run test:visual",
    "lighthouse": "lhci autorun",
    "lighthouse:local": "lighthouse http://localhost:3000 --view",
    "analyze": "ANALYZE=true next build",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel"
  },
  "dependencies": {
    "@sentry/nextjs": "^10.27.0",
    "@sparticuz/chromium": "^141.0.0",
    "@vercel/kv": "^3.0.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.554.0",
    "mercadopago": "^2.10.0",
    "next": "^14.2.33",
    "puppeteer-core": "^24.31.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "@next/bundle-analyzer": "^14.0.4",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/react": "18.3.27",
    "autoprefixer": "^10.4.16",
    "eslint-config-next": "16.0.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.4.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
